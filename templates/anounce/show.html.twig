{% extends 'base.html.twig' %}

{% block title %}{% endblock %}

{% block body %}

<div class="row">
        {% for anounc in anounce %}

        <div class="col-md-3">
            <div class="card">
                <img class="card-img-top" src="{{anounc.coverImage}}" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title"> 
                        <a href=""> {{ anounc.title }} </a>
                    </h5>
                    <p class="card-text"> {{ anounc.desription }} </p>
                </div>
            </div>
        </div>
        {% endfor %}
    </div>

{% endblock %}